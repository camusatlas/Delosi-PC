        <th>
            @Html.DisplayNameFor(model => model.nom_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tecnico_asigando)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fecha_instalacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.observaciones)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomTiendaDWH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
