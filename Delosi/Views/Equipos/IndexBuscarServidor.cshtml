            <td>
                @Html.DisplayFor(modelItem => item.join_tienda)
            </td>
            <td>
                @Html.ActionLink("Actualizar", "Edit", new { id = item.nom_tienda }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Eliminar", "Delete", new { id = item.nom_tienda }, new { @class = "btn btn-primary" })
            </td>
        </tr>
    }

</table>
