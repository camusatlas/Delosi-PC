        </th>
        <th>
            @Html.DisplayNameFor(model => model.region)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.servidor_ip)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hostname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tamaño_bd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.vers_micros)
