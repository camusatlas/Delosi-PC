@model IEnumerable<Delosi.Models.Servidor>

@{
    ViewBag.Title = "IndexBuscarServidor";
}

<h2 class="text-center">Lista de Servidores</h2>
<br />
<hr />
<div>
    @using (Html.BeginForm())
            {
                   <em>Ingrese el nomre de la tienda</em>
                   @Html.TextBox("nombre", "");
                   <button class="btn btn-success">Consultar</button>
            }
</div>
<p>
    @Html.ActionLink("Nuevo", "Create", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nom_emp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comp_tienda_servidor)
