@model IEnumerable<Delosi.Models.Kds>

@{
    ViewBag.Title = "IndexBuscarKDS";
}

<h2 class="text-center">LISTADO DE KDS</h2>
<br />
<hr />
<div>
    @using (Html.BeginForm())
    {
        <em>Ingrese el nomre de la tienda</em>
        @Html.TextBox("nombre", "");
        <button class="btn btn-success">Consultar</button>
    }
</div>
<p>
    @Html.ActionLink("Nuevo", "CreateKDS", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nom_empresa_kds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comp_tienda_kds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.num_tienda_kds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomb_teinda_kds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ip_kds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hostname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado_tienda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status_kds)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.join_tienda)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nom_empresa_kds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.comp_tienda_kds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.num_tienda_kds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomb_teinda_kds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ip_kds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hostname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.estado_tienda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.modelo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.status_kds)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.join_tienda)
            </td>
            <td>
                @*Html.ActionLink("Actualizar", "EditKDS", new { id = item.join_tienda }, new { @class = "btn btn-primary" })*@
                @*Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ })
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
            </td>
        </tr>
    }

</table>
